@model Order
<div class="block-fluid ucard">
    <div class="info">
        <ul class="rows">
            <li><strong>&nbsp;&nbsp;Billing Address</strong> </li>
            <li>
                <div class="title">
                    First Name</div>
                <div class="text">
                    @Model.BillingAddressModel.FirstName</div>
            </li>
            <li>
                <div class="title">
                    Last Name</div>
                <div class="text">
                    @Model.BillingAddressModel.LastName&nbsp;</div>
            </li>
            <li>
                <div class="title">
                    Email</div>
                <div class="text">
                    @Model.BillingAddressModel.Email &nbsp;</div>
            </li>
            <li>
                <div class="title">
                    Phone number</div>
                <div class="text">
                    @Model.BillingAddressModel.PhoneNumber &nbsp;</div>
            </li>
            <li>
                <div class="title">
                    Company</div>
                <div class="text">
                    @Model.BillingAddressModel.Company &nbsp;</div>
            </li>
            <li>
                <div class="title">
                    Address</div>
                <div class="text">
                    @Model.BillingAddressModel.Address &nbsp;</div>
            </li>
            <li>
                <div class="title">
                    State</div>
                <div class="text">
                    @Model.BillingAddressModel.State &nbsp;</div>
            </li>
            <li>
                <div class="title">
                    Country</div>
                <div class="text">
                    @Model.BillingAddressModel.GetCountryName() &nbsp;</div>
            </li>
        </ul>
    </div>
</div>
