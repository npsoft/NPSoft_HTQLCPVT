@* Activity Chart *@
@model int
<script language="javascript">
    jQuery(document).ready(function () {
        setTimeout(function () {
            if ($.plot) {
                // get chart info
                show_loading();
                jQuery.post("@Url.Action("Detail_AnalyticChart_Data", new { id = Model })", function (returnObj) {
                    hide_loading();
                    var stuff = [], contacts = [];


                    $.plot($("#website_activity_chart"), [{ data: stuff, label: "View" }, { data: contacts, label: "Order"}], { xaxis: { show: true }, yaxis: { show: true} });
                });
            }
        }, 500);
    });
</script>
<div class="mws-panel grid_5">
    <div class="mws-panel-header">
        <span><i class="icon-graph"></i>Website Activities - Google Analytic</span>
    </div>
    <div class="mws-panel-body">
        <div class="mws-panel-content">
            <div id="website_activity_chart" style="width: 100%; height: 485px;">
            </div>
        </div>
    </div>
</div>
